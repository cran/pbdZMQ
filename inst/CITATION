citHeader("To cite pbdZMQ in a publication use:")

citEntry(
  entry = "Misc",
  title = "{pbdZMQ}: Programming with Big Data -- Interface to ZeroMQ",
  author = personList(as.person("Wei-Chen Chen"),
                      as.person("Drew Schmidt")),
  year = "2015",
  note = "{R} Package, URL http://cran.r-project.org/package=pbdZMQ",
  textVersion = "Chen, W.-C. and Schmidt, P. (2015) pbdZMQ: Programming with Big Data -- Interface to ZeroMQ. R Package, URL http://cran.r-project.org/package=pbdZMQ",
  key = "Chen2015pbdZMQpackage"
)

